<?xml version="1.0"?>
<configuration>
		<configSections>
				<section name="Globals"	type="System.Configuration.DictionarySectionHandler"/>
				<section name="RelativePaths" type="System.Configuration.DictionarySectionHandler"/>
		</configSections>

		<Globals>
